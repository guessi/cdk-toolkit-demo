{
  "name": "cdk-toolkit-demo",
  "type": "commonjs",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js,json}\"",
    "clean": "rm -rf dist cdk.out",
    "prebuild": "npm run clean",
    "cleanup": "rm -rf dist cdk.out && find . -name \"*.js\" -not -path \"./node_modules/*\" -not -name \"jest.config.js\" -delete && find . -name \"*.d.ts\" -not -path \"./node_modules/*\" -delete",
    "start": "npm run build && cd lambda && npx tsc && cd .. && npx ts-node bin/lambda-test.ts",
    "update-deps": "npm update"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "aws-cdk": "^2.1101.0",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "prettier": "^3.8.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-cdk/toolkit-lib": "^1.13.0",
    "@aws-sdk/client-lambda": "^3.995.0",
    "aws-cdk-lib": "^2.235.0",
    "constructs": "^10.4.5"
  }
}